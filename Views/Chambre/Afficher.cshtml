@model List<Chambre>


<div class="container">
    <h1 class="mt-3">Chambres 2eme Etage</h1>

    <ul class="list-group mt-3">
        @foreach (Chambre chambre in ViewBag.Chambres2EmeEtage)
        {
            <li class="list-group-item d-flex justify-content-between align-items-center">
                <div>
                    <strong>Numero:</strong> @chambre.Numero |
                    <strong>Type:</strong> @chambre.Type |
                    <strong>Etage:</strong> @chambre.NumeroEtage |
                    <strong>Date Debut:</strong> @chambre.DateDebut |
                    <strong>Date Fin:</strong> @chambre.DateFin
                </div>
            </li>
        }
    </ul>

    
</div>
